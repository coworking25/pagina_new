# Configuración de SendGrid para envío de emails
# Obtén tu API key desde: https://app.sendgrid.com/settings/api_keys
VITE_SENDGRID_API_KEY=your_sendgrid_api_key_here

# Email desde el cual se enviarán los emails
VITE_FROM_EMAIL=noreply@coworkinginmobiliario.com

# Configuración de Supabase (ya existente)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Configuración de Google Calendar API
# Obtén estas credenciales desde: https://console.cloud.google.com/
# Sigue las instrucciones en GOOGLE_CALENDAR_SETUP.md
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/google/callback